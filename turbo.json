{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "type-check": {
      "dependsOn": ["^build"]
    },

    "format:fix": {
    },

    "lint:fix": {
    },

    "fix": {
    },

    "format": {
    },

    "lint": {
      "outputs": []
    },

    "start": {
      "dependsOn": ["^build"],
      "outputs": []
    },

    "test": {
      "dependsOn": ["^test"]
    },

    "build": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false
    }
  }
}
